            android:id="@+id/test9_btn12"
            style="@style/CommonButton"
            android:layout_width="fill_parent"
            android:layout_marginTop="10dp"
            android:text="doOnTerminate" />

        <Button
            android:id="@+id/test9_btn13"
            style="@style/CommonButton"
            android:layout_width="fill_parent"
            android:layout_marginTop="10dp"
            android:text="finallyDo" />

        <Button
            android:id="@+id/test9_btn14"
